<% provide(:title, @car.name) %>
<h1><%= @car.name %></h1>
<h1><%= @car.name %></h1>
<ul>
  <% if @car.photos.any? %>
    <% for photo in @car.photos %>
      <li>
        <%= image_tag(photo.image.url) %>
      </li>
    <% end %>
  <% else %>
    <%= image_tag ("default.png") %>
  <% end %>
</ul>
