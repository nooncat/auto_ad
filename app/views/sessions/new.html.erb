<% provide(:title, "Войти") %>
<div class="other-head">
  вход
</div>
<div class="row-fluid">
  <div class="sign-in">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :login, "Логин" %>
      <%= f.text_field :login %>

      <%= f.label :password, "Пароль" %>
      <%= f.password_field :password %><br />

      <%= f.submit "войти", class: "btn btn-flat" %>
    <% end %>
  </div>
</div>
