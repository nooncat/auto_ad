<% provide(:title, 'Проверка отзыва') %>
<h2>Проверка отзыва</h2>
<div class="row">
  <div class="span4 offset4">
    <%= form_for(@feedback) do |f| %>
      <%= render 'feedback_form', f: f %>
      <p><%=" Показать отзыв посетителям "%><%= f.check_box :checkf %></p>
      <%= f.submit "Сохранить отзыв", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

